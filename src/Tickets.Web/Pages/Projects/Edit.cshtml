@page
@model Tickets.Web.Pages.Projects.EditModel
@{
}

<h1>Edit</h1>


<hr />
<div class="mx-auto" style="width:1100px;">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Edit" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Project.Title" class="control-label"></label>
                    <input asp-for="Project.Title" class="form-control" />
                    <span asp-validation-for="Project.Title" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Project.ClientName" class="control-label"></label>
                    <input asp-for="Project.ClientName" class="form-control" />
                    <span asp-validation-for="Project.ClientName" class="text-danger"></span>
                </div>
                <select asp-for="Project.TypeId" asp-items="Html.GetEnumSelectList<Tickets.Domain.Entities.StatusType>()">
                    <option value=" ">Select Status</option>
                </select>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" asp-route-id="@Model.Project.Id" />
                    <a asp-page="Index" class="btn btn-outline-dark">Back to List</a>
                </div>
            </form>
        </div>
    </div>
</div>